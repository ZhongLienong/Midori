def CountingBits = fn(n : Int)
{
	def Count1 = fn(n : Int) : Int
	{
		return n == 0 ? 0 : Count1(n & n - 1) + 1;
	};

	def output = "";
	loop (def i = 0; i <= n; i = i + 1)
	{
		if output == ""
		{
			output = (Count1(i) as Text);
			continue;
		}
		output = (output as Text) ++ ", " ++ (Count1(i) as Text);
	}

	return output;
};

def main = fn()
{
	return IO::PrintLine(CountingBits(4));
};