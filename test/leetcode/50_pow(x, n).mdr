def Pow = fn(x, n)
{
	if (n < 0)
	{
		return Pow(1.0 / x, -n);
	}
	else
	{
		if (n == 0)
		{
			return 1.0;
		}
		else
		{
			if ((n & 1) == 1)
			{
				return x * Pow(x, n - 1);
			}
			else
			{
				def sub_result = Pow(x, n / 2);
				return sub_result * sub_result;
			}
		}
	}
};

def main = fn()
{
	IO::PrintLine(Pow(2.0, 10) as Text);
	IO::PrintLine(Pow(2.1, 3) as Text);
	IO::PrintLine(Pow(2.0, -2) as Text);
	return ();
};